<h1>スケジュール詳細<h1>

<table>
  <tr>
    <th>タイトル</th>
    <td><%= @schedule.title %></td>
  </tr>
  <tr>
    <th>開始日</th>
    <td><%= @schedule.start_day %></td>
  </tr>
  <tr>
    <th>終了日</th>
    <td><%= @schedule.end_day %></td>
  </tr>
  <tr>
    <th>終日</th>
    <td> <% if @schedule.allday == true %>
        <p>○</p>
      <% else %>
        <p></p>
      <% end %>
    </td>
  </tr>
  <tr>
    <th>メモ</th>
    <td><%= @schedule.memo %></td>
  </tr>
</table>

<div>
 <ul>
  <li><%= link_to"予定一覧に戻る",:schedules %></li>
 </ul>
</div>